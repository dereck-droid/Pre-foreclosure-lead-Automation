{
  "name": "OC Lis Pendens Scraper",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "cronExpression",
              "expression": "0 9 * * *"
            },
            {
              "field": "cronExpression",
              "expression": "0 14 * * *"
            }
          ]
        }
      },
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.2,
      "position": [0, 0],
      "id": "schedule-trigger",
      "name": "9am + 2pm Daily"
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "leftValue": "={{ $now.weekday }}",
              "rightValue": 5,
              "operator": {
                "type": "number",
                "operation": "lte"
              }
            }
          ],
          "combinator": "and"
        }
      },
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [240, 0],
      "id": "weekday-check",
      "name": "Is Weekday?"
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://your-server-ip:3000/scrape",
        "sendBody": false,
        "options": {
          "timeout": 300000
        }
      },
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [480, -60],
      "id": "scrape-request",
      "name": "Scrape County Website"
    }
  ],
  "connections": {
    "9am + 2pm Daily": {
      "main": [
        [
          {
            "node": "Is Weekday?",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Is Weekday?": {
      "main": [
        [
          {
            "node": "Scrape County Website",
            "type": "main",
            "index": 0
          }
        ],
        []
      ]
    }
  },
  "settings": {
    "executionOrder": "v1"
  }
}
