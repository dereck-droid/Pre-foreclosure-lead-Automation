{
  "name": "OC Lis Pendens Scraper",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "minutes",
              "minutesInterval": 10
            }
          ]
        }
      },
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.2,
      "position": [0, 0],
      "id": "schedule-trigger",
      "name": "Every 10 Minutes"
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "leftValue": "={{ $now.weekday }}",
              "rightValue": 5,
              "operator": {
                "type": "number",
                "operation": "lte"
              }
            }
          ],
          "combinator": "and"
        }
      },
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [240, 0],
      "id": "weekday-check",
      "name": "Is Weekday?"
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "leftValue": "={{ $now.hour }}",
              "rightValue": 8,
              "operator": {
                "type": "number",
                "operation": "gte"
              }
            },
            {
              "leftValue": "={{ $now.hour }}",
              "rightValue": 17,
              "operator": {
                "type": "number",
                "operation": "lte"
              }
            }
          ],
          "combinator": "and"
        }
      },
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [480, -60],
      "id": "business-hours-check",
      "name": "8am-6pm ET?"
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://your-server-ip:3000/scrape",
        "sendBody": false,
        "options": {
          "timeout": 300000
        }
      },
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [720, -120],
      "id": "scrape-request",
      "name": "Scrape County Website"
    }
  ],
  "connections": {
    "Every 10 Minutes": {
      "main": [
        [
          {
            "node": "Is Weekday?",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Is Weekday?": {
      "main": [
        [
          {
            "node": "8am-6pm ET?",
            "type": "main",
            "index": 0
          }
        ],
        []
      ]
    },
    "8am-6pm ET?": {
      "main": [
        [
          {
            "node": "Scrape County Website",
            "type": "main",
            "index": 0
          }
        ],
        []
      ]
    }
  },
  "settings": {
    "executionOrder": "v1"
  }
}
